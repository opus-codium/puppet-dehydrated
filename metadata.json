{
  "name": "opuscodium-dehydrated",
  "version": "4.1.0",
  "author": "Opus Codium",
  "summary": "Manages Let's encrypt certificates using dehydrated",
  "license": "MIT",
  "source": "https://github.com/opus-codium/puppet-dehydrated",
  "project_page": "https://github.com/opus-codium/puppet-dehydrated",
  "issues_url": "https://github.com/opus-codium/puppet-dehydrated/issues",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 2.1.0 < 8.0.0"
    },
    {
      "name": "puppetlabs/ruby_task_helper",
      "version_requirement": ">= 0.6.0 < 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ],
  "description": "The dehydrated module lets you use Puppet to manage Let's Encrypt certificates creation and renewal using dehydrated."
}
